<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookApp">
        <activity android:name=".WebsiteActivity"></activity>
        <activity
            android:name=".FavoritesActivity"
            android:label="@string/books_favorite_label" />
        <activity
            android:name=".CurrentlyReadingActivity"
            android:label="@string/books_currently_reading" />
        <activity
            android:name=".WantToReadBookActivity"
            android:label="@string/books_want_to_read_label" />
        <activity
            android:name=".AlreadyReadBookActivity"
            android:label="@string/books_already_read_label" />
        <activity
            android:name=".BookDetailsActivity"
            android:label="@string/book_details_label" />
        <activity
            android:name=".AllBooksActivity"
            android:label="@string/all_books_label" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>